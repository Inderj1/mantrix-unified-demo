# ============================================================================
# Semantic Term: Payment Run
# ============================================================================

ap:term_payment_run a mantrix:SemanticTerm ;
    rdfs:label "payment run" ;
    rdfs:comment "SAP F110 automatic payment program. Selects due/eligible invoices, groups by vendor, creates payment documents, and generates payment media (checks, wires, ACH)." ;
    skos:altLabel "F110" , "payment proposal" , "auto pay" , "disbursement run" ;
    mantrix:canonicalEntity "REGUH_PaymentHeaders, REGUP_PaymentItems" ;
    mantrix:disambiguation """Payment run process:
1. F110 selects open items where: due date <= run date, no payment block, vendor not blocked
2. Creates payment proposal (REGUH = header per vendor, REGUP = items per invoice)
3. On execution: creates payment documents (VBLNR), clears open items (sets AUGBL in ACDOCA)
4. Generates payment media (check print, wire file, ACH file)""" ;
    mantrix:relatedTerms "REGUH, REGUP, F110, blocked, due date, clearing" .
