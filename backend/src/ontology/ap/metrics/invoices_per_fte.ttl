@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ap: <http://mantrix.ai/ontology/ap#> .
@prefix mantrix: <http://mantrix.ai/ontology#> .

ap:metric_invoices_per_fte a mantrix:Metric ;
    rdfs:label "Invoices per FTE" ;
    rdfs:comment "Number of invoices processed per full-time equivalent AP clerk. Measures AP team productivity and automation effectiveness." ;
    skos:altLabel "productivity" , "invoices per person" , "AP efficiency" ;
    mantrix:category "productivity" ;
    mantrix:unit "count" ;
    mantrix:direction "higher_is_better" ;
    mantrix:benchmark "10,000-15,000/year (best-in-class), 5,000-8,000/year (average)" ;
    mantrix:sourceTables "RBKP_InvoiceHeaders" ;
    mantrix:edgeCases "FTE count is typically not in SAP data. This metric requires external staffing data combined with invoice volume." .
