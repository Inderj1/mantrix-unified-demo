@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix copa: <http://mantrix.ai/ontology/copa#> .
@prefix mantrix: <http://mantrix.ai/ontology#> .

copa:metric_net_margin a mantrix:Metric ;
    rdfs:label "Net Margin" ;
    rdfs:comment "Net profit margin percentage. Sales margin relative to net sales." ;
    skos:altLabel "net profit margin" , "profit margin" , "NPM" ;
    mantrix:sqlFormula "SUM(Sales_Margin_of_Net_Sales) / NULLIF(SUM(Net_Sales), 0) * 100" ;
    mantrix:exampleSQL "SELECT SUM(Sales_Margin_of_Net_Sales) / NULLIF(SUM(Net_Sales), 0) * 100 as net_margin_pct FROM dataset_25m_table" .
