@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix copa: <http://mantrix.ai/ontology/copa#> .
@prefix mantrix: <http://mantrix.ai/ontology#> .

copa:dim_brand a mantrix:Dimension ;
    rdfs:label "Brand" ;
    rdfs:comment "Brand dimension. Groups products into brand families for portfolio analysis." ;
    skos:altLabel "brand" , "product line" , "brand family" ;
    mantrix:bqColumns "Brand" ;
    mantrix:exampleQuestion "Revenue by brand" ;
    mantrix:exampleSQL "SELECT Brand, SUM(Gross_Revenue) as revenue, SUM(Net_Sales) as net, SUM(Cogs) as cogs FROM dataset_25m_table GROUP BY Brand ORDER BY revenue DESC" ;
    mantrix:exampleQuestion "Brand market share" ;
    mantrix:exampleSQL "SELECT Brand, SUM(Net_Sales) as net_sales, SUM(Net_Sales) / (SELECT SUM(Net_Sales) FROM dataset_25m_table) * 100 as share_pct FROM dataset_25m_table GROUP BY Brand ORDER BY share_pct DESC" .
