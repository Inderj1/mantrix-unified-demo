@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix copa: <http://mantrix.ai/ontology/copa#> .
@prefix mantrix: <http://mantrix.ai/ontology#> .

copa:dim_company_code a mantrix:Dimension ;
    rdfs:label "Company Code" ;
    rdfs:comment "Legal entity for financial reporting in COPA." ;
    skos:altLabel "company" , "entity" , "BUKRS" ;
    mantrix:bqColumns "Company_Code" ;
    mantrix:exampleSQL "SELECT Company_Code, SUM(Net_Sales) as revenue, SUM(Cogs) as cogs FROM dataset_25m_table GROUP BY Company_Code" .
